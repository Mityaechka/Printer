{
  "name": "Getting Started Example",
  "version": "1.0",
  "description": "Build an Extension!",
  "permissions": ["storage","nativeMessaging","declarativeContent","activeTab"],
  "background": {
    "scripts": ["printerCommands.js","ReceiptBuilder.js","background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "manifest_version": 2,
  "externally_connectable": {
    "matches": ["<all_urls>"]
  }
}